		lang: en
		tempDir: %appDir%/../temp
		logDir: %appDir%/../log
		
	mail:
		from: tatami@tatami.local
		fromName: Tatami
		mailer: smtp
		smtp:
			secure: ssl
			host: smtp.gmail.com
			port: "465"
			username: martin@bazo.sk
			password: kavicka666
			
		
	services:
		robotLoader:
			option:
				directory:
					- %appDir%
					- %libsDir%
					
				
			run: true
			
		authenticator:
			factory: \Tatami\Security\Security::initialize
			
		authorizator:
			factory: \Tatami\Security\Acl::create
			
		presenterFactory:
			factory: \Tatami\ServiceFactories\PresenterFactory::create
			
		translator: \Tatami\Tools\Translator
		entityManager:
			factory: \Tatami\ServiceFactories\EntityManagerFactory::create
			
		eventManager:
			factory: \Tatami\ServiceFactories\EventManagerFactory::create
			
		moduleManager:
