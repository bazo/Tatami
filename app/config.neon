		shortcutsManager:
			factory: \Tatami\ServiceFactories\ShortcutsManagerFactory::create
			
		mailer:
			factory: Tatami\ServiceFactories\MailerFactory::create
			
		
	memcache:
		host: false
		port: false
		
	database:
		driver: pdo_mysql
		host: localhost
